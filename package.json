{
  "name": "vue-flux",
  "version": "1.0.0",
  "description": "Vue image slider",
  "author": "ragnar lotus",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deulos/vue-flux.git"
  },
  "keywords": [
    "vue",
    "image",
    "slider"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deulos/vue-flux/issues"
  },
  "homepage": "https://github.com/deulos/vue-flux",
  "main": "dist/vue-flux.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-docs": "vue-cli-service build --dest docs",
    "bundle": "vue-cli-service build --target wc --name vue-flux \"src/components/*.vue\""
  },
  "dependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.16",
    "@vue/cli-service": "^3.0.0-beta.16",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "vue": {
    "baseUrl": "/vue-flux/"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
